{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "lib": ["es6", "dom", "es2017"]
  }
}
